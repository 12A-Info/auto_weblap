<%- include('partials/header_02.ejs') -%>
<a href="/">Vissza a főoldalra</a>
<form>
    <input
        type="hidden"
        id="azon"
        value="<%= dudu[0].id %>"
    />
    <label for="marka">Márka: </label>
    <input
        type="text"
        id="marka"
        value="<%= dudu[0].marka %>"
    />
    <label for="tipus">Típus: </label>
    <input
        type="text"
        id="tipus"
        value="<%= dudu[0].tipus %>"
    />
    <label for="ar">Ár: </label>
    <input
        type="number"
        id="ar"
        value="<%= dudu[0].ar %>"
    />
    <label for="kep">Kép:</label>
    <input
        type="text"
        id="kep"
        value="<%= dudu[0].kep %>"
    />
    <button onclick="modosit(event)">Módosítás</button>
</form>
<script src="../js/autokezeles.js"></script>
<%- include('partials/footer.ejs') -%>
